        <Button x:Name="sept" Click="Button_Click" Content="7" HorizontalAlignment="Left" Margin="10,120,0,0" VerticalAlignment="Top" Width="45" Height="45" FontSize="24" Background="Gray"/>
        <Button x:Name="huit" Click="Button_Click" Content="8" HorizontalAlignment="Left" Margin="65,120,0,0" VerticalAlignment="Top" Width="45" Height="45" FontSize="24" Background="Gray"/>
        <Button x:Name="neuf" Click="Button_Click" Content="9" HorizontalAlignment="Left" Margin="120,120,0,0" VerticalAlignment="Top" Width="45" Height="45" FontSize="24" Background="Gray"/>
    </Grid>

</Window>
